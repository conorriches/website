{
  "name": "gatsby-universal",
  "license": "MIT",
  "author": "Fabian Schultz <desk@fabianschultz.com>",
  "scripts": {
    "add-domain": "echo conorriches.co.uk > ./public/CNAME",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose",
    "deploy": "gatsby build --prefix-paths && npm run add-domain && gh-pages -d public"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "core-js"
    ]
  },
  "dependencies": {
    "@reach/dialog": "^0.2.0",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.1.1",
    "babel-plugin-styled-components": "^2.0.6",
    "classnames": "^2.2.6",
    "gatsby": "^4.9.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-offline": "^5.9.0",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sass": "^5.9.0",
    "gatsby-plugin-sharp": "^4.9.0",
    "gatsby-plugin-sitemap": "^5.9.0",
    "gatsby-plugin-styled-components": "^5.9.0",
    "gatsby-plugin-webpack-size": "^2.0.1",
    "gatsby-remark-figure-caption": "^2.0.0",
    "gatsby-remark-images": "^6.9.0",
    "gatsby-remark-prismjs": "^7.3.0",
    "gatsby-remark-twitter-cards": "^0.6.1",
    "gatsby-source-filesystem": "^4.9.0",
    "gatsby-transformer-json": "^4.9.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^4.9.0",
    "intersection-observer": "^0.5.1",
    "postcss": "^8.4.8",
    "prismjs": "^1.29.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-pose": "^4.0.5",
    "sharp": "^0.31.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "ava": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-react": "^6.24.1",
    "chrome-launcher": "^0.15.0",
    "core-js": "^3.21.1",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.3",
    "favicons": "^6.2.2",
    "gatsby-plugin-eslint": "^4.0.2",
    "gh-pages": "^2.1.1",
    "lighthouse": "^9.4.0",
    "mkdirp": "^0.5.1",
    "node-gyp": "^9.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.49.9"
  }
}
